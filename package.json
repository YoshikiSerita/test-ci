{
  "name": "test-ci",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next build && next export",
    "serve": "firebase emulators:start --only hosting --project test",
    "lint": "next lint --dir src --dir cypress --dir .storybook",
    "tsc": "tsc --noEmit --incremental false",
    "depcruise": "depcruise --config .dependency-cruiser.js src",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "e2e": "cypress open --e2e -b chrome",
    "e2e:run": "cypress run",
    "gen:api": "openapi-generator-cli generate -g typescript-axios -t ./oapi-generator-template -i ./submodules/api/reference/Kokoroe.v1.yaml -o ./src/infrastructure/api --global-property skipFormModel=false",
    "mock:api": "prism mock ./submodules/api/reference/Kokoroe.v1.yaml",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "test-storybook-ci": "npm run build-storybook && npx concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npx http-server storybook-static --port 6006 --silent\" \"npx wait-on tcp:6006 && npm run test-storybook\"",
    "watch:path": "pathpida --ignorePath .gitignore --output src/utils --watch",
    "generate:path": "pathpida --ignorePath .gitignore --output src/utils",
    "prune": "ts-prune"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@fullcalendar/core": "^6.1.9",
    "@fullcalendar/daygrid": "^6.1.9",
    "@fullcalendar/react": "^6.1.9",
    "@mui/icons-material": "^5.14.8",
    "@mui/lab": "^5.0.0-alpha.143",
    "@mui/material": "^5.14.8",
    "@mui/utils": "^5.14.8",
    "@newrelic/browser-agent": "^1.239.1",
    "@u-wave/react-vimeo": "^0.9.10",
    "axios": "^0.21.1",
    "clsx": "^1.1.1",
    "csv-parse": "^4.16.3",
    "date-fns": "^2.30.0",
    "firebase": "^9.1.3",
    "next": "^13.4.19",
    "papaparse": "^5.3.1",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.30.0",
    "react-query": "^3.39.2",
    "recharts": "^2.8.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.4.26",
    "@stoplight/prism-cli": "^4.10.3",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/nextjs": "^7.4.0",
    "@storybook/react": "^7.4.0",
    "@storybook/test-runner": "^0.13.0",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^14.14.31",
    "@types/papaparse": "^5.2.6",
    "@types/react": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "cypress": "^13.6.3",
    "dependency-cruiser": "^12.9.0",
    "eslint": "^8.48.0",
    "eslint-config-next": "^13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.14.0",
    "eslint-plugin-storybook": "^0.6.13",
    "firebase-tools": "^13.0.3",
    "husky": "^5.0.9",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "jest-localstorage-mock": "^2.4.26",
    "pathpida": "^0.22.0",
    "prettier": "^3.0.3",
    "storybook": "^7.4.0",
    "ts-node": "^10.8.1",
    "ts-prune": "^0.10.3",
    "typescript": "^5.3.3"
  },
  "overrides": {
    "@testing-library/cypress": {
      "cypress": "^13.0.0"
    }
  },
  "msw": {
    "workerDirectory": "public"
  },
  "volta": {
    "node": "20.10.0"
  }
}
